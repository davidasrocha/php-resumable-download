{
    "name": "davidasrocha/php-resumable-download",
    "type": "library",
    "description": "PHP Resumable Download library that add supports to consume servers with HTTP Partial Request",
    "keywords": [
        "php",
        "http",
        "partial request",
        "resume download",
        "download"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "David Anderson",
            "email": "david.anderson.sil@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.4",
        "monolog/monolog": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "PHP\\ResumableDownload\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "@stable"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
